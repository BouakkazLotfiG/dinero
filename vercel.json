{
    "builds": [
      {
        "src": "dinero/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "use": "@vercel/builders/inline",
        "src": "pip install -r requirements.txt && python manage.py makemigrations && python manage.py migrate"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dinero/wsgi.py"
      }
    ]
  }
  